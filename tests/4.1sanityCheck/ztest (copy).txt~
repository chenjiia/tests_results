var S = 0;

var SMax = 128;

var STemp = S;
var OTemp = 0;
var OCounter = 0;
var base = 4;
var OMax = 0;

//code start
if(STemp < 16){
	OTemp = base+ STemp;
}
else{
	OTemp = base;
}
//code end

OMax = OTemp;
OCounter = 1;
S = S + 1;


while(S != SMax){
	STemp = S;
	//code start
	if(STemp < 16){
		OTemp = base+ STemp;
	}
	else{
		OTemp = base;
	}
	//code end

	if (OTemp > OMax ){
		OMax = OTemp; 
		OCounter= OCounter+1;
	}
	S = S + 1;
}

if(OCounter == 16){
	reach: 0>1;
}
