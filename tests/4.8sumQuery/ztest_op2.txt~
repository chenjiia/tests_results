var S1 = 0;
var S2 = 0;
var S3 = 0;

var SMax = 10;

var STemp = S;
var OTemp = 0;
var OCounter = 0;
var OMax = 0;

//code start		
OTemp = S1 + S2;
OTemp = OTemp + S3;
//code end

OMax = OTemp;
OCounter = 1;
S3 = S3 + 1;


while(S1 != SMax){
	S1=0;
	while(S2 != SMax){
		S2=0;
		while(S3 != SMax){
			OTemp = S1 + S2;
			OTemp = OTemp + S3;

			if (OTemp > OMax ){
				OMax = OTemp; 
				OCounter= OCounter+1;
			}
			S3=S3+1;
		}
		S2=S2+1;
	}
	
	S1 = S1 + 1;
}

if(OCounter == 28){
	reach: 0>1;
}
